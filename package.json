{
  "name": "watch-with-me",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajax": "0.0.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "mongodb": "^3.2.1",
    "mongoose": "^5.4.19",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve-dev": "ENV NODE_ENV=development nodemon -w server --inspect --exec \"babel-node server --presets es2015,stage-0\"",
    "watch": "npm-watch",
    "build-css": "node-sass -o css css",
    "watch-css": "nodemon -e sass -x \"npm run build-css\"",
    "start-dev": "npm-run-all start serve-dev watch build-css watch-css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-watch": "^0.6.0"
  },
  "proxy": "http://localhost:8080",
  "watch": {
    "build": "src/"
  }
}
